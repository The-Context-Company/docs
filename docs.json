{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "The Context Company",
  "colors": {
    "primary": "#197ef7",
    "light": "#197ef7",
    "dark": "#197ef7"
  },
  "appearance": {
    "default": "light"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "concepts", "contact"]
          },
          {
            "group": "Supported frameworks",
            "pages": [
              {
                "group": "Vercel AI SDK",
                "pages": [
                  "frameworks/ai-sdk/setup",
                  "frameworks/ai-sdk/usage",
                  "frameworks/ai-sdk/local"
                ]
              },
              "frameworks/langchain",
              "frameworks/langgraph"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/logo.png",
    "dark": "/logo/logo.png"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://thecontext.company/runs"
    }
  },
  "contextual": {
    "options": ["copy", "chatgpt", "claude", "cursor"]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/thecontextco",
      "linkedin": "https://www.linkedin.com/company/the-context-company"
    }
  },
  "fonts": {
    "family": "Poppins"
  },
  "redirects": [
    {
      "source": "/frameworks/vercel",
